= button_tag "Generate debug Sample Data", :id => 'unhide_sample_data_form', :class => "btn btn-primary"

= form_tag sensor_readings_debug_report_sensor_path(@report, @sensor.id), :id => 'sample_data_form', :method => "post", :remote => true do
  = fields_for :sample do |f|
    = f.label :quantity, "Quantity"
    = f.number_field :quantity
    = f.label :from, "From"
    = f.number_field :from
    = f.label :to, "To"
    = f.number_field :to
    = f.submit "Generate!"
